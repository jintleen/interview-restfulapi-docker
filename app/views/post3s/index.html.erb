<p id="notice"><%= notice %></p>

<h1>Listing Post3s</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @post3s.each do |post3| %>
      <tr>
        <td><%= post3.title %></td>
        <td><%= post3.content %></td>
        <td><%= link_to 'Show', post3 %></td>
        <td><%= link_to 'Edit', edit_post3_path(post3) %></td>
        <td><%= link_to 'Destroy', post3, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Post3', new_post3_path %>
